{
	"name": "typescript-api-project",
	"version": "1.0.0",
	"main": "dist/index.js",
	"scripts": {
		"build": "tsc",
		"start": "node dist/index.js",
		"dev": "tsx src/index.ts",
		"test": "vitest",
		"test:coverage": "vitest run --coverage",
		"db:init": "tsx src/init-db.ts --seed",
		"db:migrate": "tsx src/init-db.ts",
		"db:seed": "tsx -e \"import('./src/db/seed').then(m => m.seedDatabase())\""
	},
	"dependencies": {
		"@types/bcrypt": "^6.0.0",
		"@types/cookie-parser": "^1.4.9",
		"@types/cors": "^2.8.17",
		"@types/express-session": "^1.18.2",
		"@types/sqlite3": "^3.1.11",
		"@types/swagger-jsdoc": "^6.0.4",
		"@types/swagger-ui-express": "^4.1.8",
		"bcrypt": "^6.0.0",
		"cookie-parser": "^1.4.7",
		"cors": "^2.8.5",
		"express": "^4.21.2",
		"express-rate-limit": "^8.1.0",
		"express-session": "^1.18.2",
		"express-validator": "^7.2.1",
		"helmet": "^8.1.0",
		"sqlite3": "^5.1.7",
		"swagger-jsdoc": "^6.2.8",
		"swagger-ui-express": "^5.0.1"
	},
	"devDependencies": {
		"@types/express": "^5.0.0",
		"@types/node": "^22.12.0",
		"@types/supertest": "6.0.2",
		"@vitest/coverage-v8": "3.0.5",
		"supertest": "7.0.0",
		"tsx": "^4.20.5",
		"typescript": "^5.7.3",
		"vitest": "3.0.5"
	},
	"license": "MIT"
}
